<div id='polygon-template' class="panel panel-default" >
  <div class="panel-heading">
    <h4 class="panel-title row">
      <span class='col-xs-8'>
        <input type='text' class='form-control polygon-name' value='Coordenadas extremas' />
      </span>
      <span class='col-xs-4 poligon-icons'>
        <a data-toggle="collapse" href="#collapseOne" class='glyphicon glyphicon-plus pull-right collapser' title='abrir'></a>
        <a data-toggle="tooltip" data-placement="top" title="Guardar" class='glyphicon glyphicon-save pull-right ttip save' href='#' ></a>
        <a data-toggle="tooltip" data-placement="top" title="Poligono Principal" class='glyphicon glyphicon-star-empty pull-right ttip set-primary-polygon' href='#'></a>
      </span>
    </h4>
  </div>
  <div id="collapseOne" class="panel-collapse collapse">
     <table class='table'>
        <tr><th>x</th><th>y</th><th></th></tr>
        <tr ng-repeat='point in space.points' >
          <td>{{point.x}}</td>
          <td>{{point.y}}</td>
          <td><a href='#' class='glyphicon glyphicon-remove'></a></td>
        </tr>
      </table>
  </div>
</div>